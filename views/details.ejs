<body>
	<a href="/accounts"><p>&larr;</p></a>
	<div id="box-container-1">
		
		<div id="box-container-2">
			<div id="box-1">
			
				<div id="box-1-1">
					<h3>ID<br>
						<h4>
							<%=data1.ID%>
						</h4>	
					</h3>
				</div>
				
				<div id="box-1-2">
					<h3>NAME<br>
						<h4>
							<%=data1.Name%>	
						</h4>		
					</h3>
				</div>
					
			
				<div id="box-1-3">
					<h5>AGE<br>
						<h4>
							<%=data1.age%>
						</h4>
				</h5>
				</div>
				
				<div id="box-1-4">
					<h5>CITY<br>
						<h4>
							<%=data1.city%>
						</h4>
					</h5>
				</div>
				
					
		</div>
		
		
 		
		<div id="box-2">
			<div id="d3">
				<h3>TOTAL<br>
				<h4>
					<%=data1.Total%>
				</h4>
				
			</h3>
			</div>
			
			<div id="box-2-1">
				<div id="1">
					<h5>PAID <br><h4>
						<%=data1.Paid%>
						</h4></h5>
				</div>
				<div id="2">
					<h5>DUE<br>  <h4>
						<%=data1.Due%>
						</h4></h5>
				</div>
			</div>
				
			
		</div>
		</div>
  		
	

	
	
	<div id="table">
		<table>
		<tr>
		<td><b>ID</b></td><td><b>TOTAL</b></td><td><b>PAID</b></td><td><b>DUE</b></td><td><b>DATE</b></td>
		</tr>
		<%
        if(data.length!=0){
        var i=1;
        data.forEach(function(data){
        %>
        <tr>
            <td><%=data.id %></td>
            <td><%=data.total %></td>
            <td><%=data.paid %></td>
            <td><%=data.due %></td>
			<td><%=data.date_time %></td>
        </tr>
        <%  i++; }) %>
        <% } else{ %>
            <tr>
                <td colspan="7">No Data Found</td>
            </tr>
        <% } %>
		</table>
	</div>
	
	</div>
</body>







